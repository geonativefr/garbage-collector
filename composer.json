{
    "name": "geonative/garbage-collector",
    "description": "Doctrine Entities garbage collector",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Benoit POLASZEK",
            "email": "benoit.polaszek@geonative.fr"
        },
        {
            "name": "MickaÃ«l ISAERT",
            "email": "mickael.isaert@geonative.fr"
        }
    ],
    "require": {
        "php": ">=8.3",
        "symfony/http-kernel": "^7.4 || ^8.0",
        "symfony/console": "^7.4 || ^8.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "symfony/uid": "^7.4 || ^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.3",
        "symfony/yaml": "^7.4 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "GeoNative\\GarbageCollector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeoNative\\GarbageCollector\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
